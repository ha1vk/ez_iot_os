
#
# Toolchain configuration
#
CONFIG_TOOLCHAIN_PATH="/opt/toolchain/esp32/tools/xtensa-esp32-elf/esp-2019r2-8.2.0/xtensa-esp32-elf/bin/"
CONFIG_TOOLCHAIN_PREFIX="xtensa-esp32-elf-"
# end of Toolchain configuration

#
# Ez-iot configuration
#

#
# ez-iot core configuration
#
# CONFIG_EZIOT_CORE_ENABLE is not set
# end of ez-iot core configuration

#
# ez-iot base configuration
#
# CONFIG_EZIOT_BASE_ENABLE is not set
# end of ez-iot base configuration

#
# ez-iot hub configuration
#
# CONFIG_EZIOT_HUB_ENABLE is not set
# end of ez-iot hub configuration

#
# ez-iot model configuration
#
# CONFIG_EZIOT_MODEL_ENABLE is not set
# end of ez-iot model configuration

#
# ez-iot ota configuration
#
# CONFIG_EZIOT_OTA_ENABLE is not set
# end of ez-iot ota configuration

#
# ez-iot shadow configuration
#
CONFIG_EZIOT_SHADOW_ENABLE=y
CONFIG_EZIOT_SHADOW_TASK_PRIORITY=2
CONFIG_EZIOT_SHADOW_STACK_SIZE=16384
CONFIG_EZIOT_SHADOW_FULL_SYNC_RETRY_MAX=3
CONFIG_EZIOT_SHADOW_FULL_SYNC_RETRY_INTERVAL=30
CONFIG_EZIOT_SHADOW_FORCE_FULL_SYNC_INTERVAL=86400
# end of ez-iot shadow configuration

#
# ez-iot TSL(Thing Specification Language) configuration
#
# CONFIG_EZIOT_TSL_ENABLE is not set
# end of ez-iot TSL(Thing Specification Language) configuration
# end of Ez-iot configuration

#
# Ez-os bsp configuration
#
CONFIG_EZOS_KV_ENABLE=y
CONFIG_EZOS_LIBC_ENABLE=y
CONFIG_EZOS_MEM_ENABLE=y
CONFIG_EZOS_SOCKET_ENABLE=y
CONFIG_EZOS_SYSTEM_ENABLE=y
CONFIG_EZOS_THREAD_ENABLE=y
CONFIG_EZOS_TIME_ENABLE=y
# end of Ez-os bsp configuration

#
# Components configuration
#

#
# Component FlashDB configuration
#
# CONFIG_EZIOT_COMPONENT_FLASHDB_ENABLE is not set
# end of Component FlashDB configuration

#
# Component cJSON configuration
#
CONFIG_EZIOT_COMPONENT_CJSON_ENABLE=y
# end of Component cJSON configuration

#
# Component double_link_list configuration
#
# CONFIG_EZIOT_COMPONENT_DOUBLE_LINK_LIST_ENABLE is not set
# end of Component double_link_list configuration

#
# Component ezconn configuration
#
CONFIG_EZIOT_COMPONENT_EZCONN_ENABLE=y
CONFIG_EZIOT_COMPONENT_EZCONN_HTTPD_STACK_SIZE=8192
# end of Component ezconn configuration

#
# Component ezlist configuration
#
CONFIG_EZIOT_COMPONENT_EZLIST_ENABLE=y
# end of Component ezlist configuration

#
# Component ezlog configuration
#
CONFIG_EZIOT_COMPONENT_EZLOG_ENABLE=y
CONFIG_EZIOT_COMPONENT_EZLOG_OUTPUT_ENABLE=y
# CONFIG_EZIOT_COMPONENT_EZLOG_ASSERT_ENABLE is not set
CONFIG_EZIOT_COMPONENT_EZLOG_LINE_BUF_SIZE=1024
CONFIG_EZIOT_COMPONENT_EZLOG_LINE_NUM_MAX_LEN=5
CONFIG_EZIOT_COMPONENT_EZLOG_FILTER_TAG_MAX_LEN=16
CONFIG_EZIOT_COMPONENT_EZLOG_FILTER_KW_MAX_LEN=16
CONFIG_EZIOT_COMPONENT_EZLOG_FILTER_TAG_LVL_MAX_NUM=5
CONFIG_EZIOT_COMPONENT_EZLOG_NEWLINE_SIGN_CRLF=y
# CONFIG_EZIOT_COMPONENT_EZLOG_NEWLINE_SIGN_LF is not set
# CONFIG_EZIOT_COMPONENT_EZLOG_NEWLINE_SIGN_CR is not set
CONFIG_EZIOT_COMPONENT_EZLOG_COLOR_ENABLE=y
# CONFIG_EZIOT_COMPONENT_EZLOG_ASYNC_OUTPUT_ENABLE is not set
# CONFIG_EZIOT_COMPONENT_EZLOG_BUF_OUTPUT_ENABLE is not set
# end of Component ezlog configuration

#
# Component eztimer configuration
#
CONFIG_EZIOT_COMPONENT_EZTIMER_ENABLE=y
CONFIG_EZIOT_CONPONENT_TIMER_TASK_STACK_SIZE=4096
# end of Component eztimer configuration

#
# Component ezutil configuration
#
# CONFIG_EZIOT_COMPONENT_EZUTIL_LST_ENABLE is not set
# CONFIG_EZIOT_COMPONENT_EZUTIL_MISC_ENABLE is not set
# CONFIG_EZIOT_COMPONENT_EZUTIL_S2J_ENABLE is not set
# CONFIG_EZIOT_COMPONENT_EZUTIL_UUID_ENABLE is not set
# end of Component ezutil configuration

#
# Component ezxml configuration
#
# CONFIG_EZIOT_COMPONENT_EZXML_ENABLE is not set
# end of Component ezxml configuration

#
# Component http_server configuration
#
CONFIG_EZIOT_COMPONENT_EZHTTPD_ENABLE=y
# end of Component http_server configuration

#
# component mbedtls configuration
#
# CONFIG_EZIOT_COMPONENT_MBEDTLS_ENABLE is not set
# end of component mbedtls configuration

#
# Component mqtt configuration
#
# CONFIG_EZIOT_COMPONENT_MQTT_ENABLE is not set
# end of Component mqtt configuration

#
# Component utest configuration
#
# CONFIG_EZIOT_COMPONENT_UTEST_ENABLE is not set
# end of Component utest configuration

#
# Component webclient configuration
#
# CONFIG_EZIOT_COMPONENT_WEBCLIENT_ENABLE is not set
# end of Component webclient configuration
# end of Components configuration

#
# Unit-Test configuration
#
# CONFIG_EZIOT_UNIT_TEST_ENABLE is not set
CONFIG_EZIOT_UNIT_TEST_DEV_DISPLAY_NAME="IOT_UTEST_DEV"
CONFIG_EZIOT_UNIT_TEST_DEV_FIRMWARE_VERSION="V1.2.0 build 201212"
# end of Unit-Test configuration
